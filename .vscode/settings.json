{
    "editor.accessibilitySupport": "off",
    "terminal.integrated.defaultProfile.osx": "zsh",
    "editor.acceptSuggestionOnCommitCharacter": false,
    "editor.quickSuggestions": {
        "comments": "off",
        "strings": "off",
        "other": "off"
    },
    "editor.cursorWidth": 7,
    "editor.minimap.enabled": false,
    "workbench.colorTheme": "Visual Studio Dark",
    "workbench.colorCustomizations": {
        "editor.foreground": "#D1B897",
    },
    "editor.tokenColorCustomizations": {
        "numbers": "#CE9178",
        "keywords": "#CE9178",
        "textMateRules": [
            {
                "scope": "punctuation",
                "settings": {
                    "foreground": "#CE9178"
                }
            },
            {
                "scope": "storage.modifier",
                "settings": {
                    "foreground": "#CE9178"
                }
            },
            {
                "scope": "keyword.other.typedef.c",
                "settings": {
                    "foreground": "#CE9178"
                }
            },
            {
                "scope": "storage.modifier.array.bracket.square.c",
                "settings": {
                    "foreground": "#CE9178",
                    "fontStyle": "bold"
                }
            }
        ]
    },
    "redhat.telemetry.enabled": true,
    "explorer.confirmDelete": false,
    "hexeditor.columnWidth": 16,
    "hexeditor.showDecodedText": true,
    "hexeditor.defaultEndianness": "little",
    "hexeditor.inspectorType": "aside",
    "C_Cpp.vcFormat.space.pointerReferenceAlignment": "ignore",
    "C_Cpp.formatting": "vcFormat",
    "C_Cpp.vcFormat.newLine.closeBraceSameLine.emptyFunction": true,
    "C_Cpp.vcFormat.newLine.scopeBracesOnSeparateLines": true,
    "C_Cpp.vcFormat.indent.multiLineRelativeTo": "outermostParenthesis",
    "editor.formatOnSave": true,
    "C_Cpp.enhancedColorization": "disabled",
    "editor.bracketPairColorization.enabled": false,
    "C_Cpp.dimInactiveRegions": false,
    "window.zoomLevel": 2,
    "files.associations": {
        "vector": "cpp",
        "iostream": "cpp",
        "list": "cpp",
        "iosfwd": "cpp",
        "iterator": "cpp",
        "new": "cpp",
        "string": "c",
